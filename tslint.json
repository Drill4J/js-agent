{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "jsRules": {},
  "rules": {
    "prettier": true,
    "quotemark": [true, "single"],
    "radix": false,
    "object-literal-sort-keys": false,
    "trailing-comma": [false],
    "no-console": {
      "severity": "warning",
      "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
    }
  },
  "rulesDirectory": []
}
