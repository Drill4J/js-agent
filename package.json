  "devDependencies": {
    "@types/convert-source-map": "^1.5.1",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^24.0.25",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.4.1",
    "@types/node": "^13.1.4",
    "@types/supertest": "^2.0.8",
