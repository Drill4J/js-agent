{
  "name": "agent-js",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "eslint . --ext .ts",
    "build": "tsc -p .",
    "predev": "npm run build",
    "dev": "npx nodemon dist/index.js",
    "test": "jest --coverage --colors",
    "start": "node .",
    "lint": "eslint . --ext .ts --fix",
    "debug": "npx nodemon --inspect dist/index.js"
  },
  "keywords": [],
  "author": "",
